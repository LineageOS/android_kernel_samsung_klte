&soc {
	tspp: msm_tspp@f99d8000 {
		status = "okay";
	};


	isdbt_fc8300_data {
		compatible = "isdb_fc8300_pdata";
		qcom,isdb-gpio-pwr-en = <&msmgpio 81 0>;
		qcom,isdb-gpio-rst = <&msmgpio 79 0>;
		qcom,isdb-gpio-irq = <&msmgpio 74 0>;
		qcom,isdb-gpio-i2c_sda = <&msmgpio 87 0>;
		qcom,isdb-gpio-i2c_scl = <&msmgpio 88 0>;
		qcom,isdb-bbm-xtal-freq = <26000>;
	};

};
