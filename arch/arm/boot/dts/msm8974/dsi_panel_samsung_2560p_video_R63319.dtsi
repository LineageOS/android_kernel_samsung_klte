		qcom,mdss-pan-porch-values = <64 20 150 8 4 12>;  /* <HBP> <HPW> <HFP> <VBP> <VPW> <VFP> */
		qcom,mdss-pan-underflow-clr = <0xff>;
		qcom,mdss-pan-bl-ctrl = "UNKNOWN_CTRL";

		qcom,mdss-pan-dsi-mode = <0>; /* VIDEO = 0, CMD = 1*/
