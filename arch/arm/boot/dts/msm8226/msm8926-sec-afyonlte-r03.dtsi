		qcom,cdc-mclk-gpios = <&pm8226_gpios 1 0>;
		qcom,cdc-lineout-spkr-gpios = <&msmgpio 23 0>;
		qcom,cdc-vdd-spkr-gpios = <&msmgpio 24 0>;
	};

	battery {
		status = "okay";
